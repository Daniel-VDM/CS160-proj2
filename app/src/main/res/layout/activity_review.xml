<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Review">

    <Button
        android:id="@+id/Submit"
        android:layout_width="165dp"
        android:layout_height="63dp"
        android:backgroundTint="@color/colorPrimary"
        android:text="SUBMIT"
        android:textColor="@android:color/white"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/floatingActionButton2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/AddressReview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2165 Kittredge St, Berkeley, CA 94704"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/NameReview"
        app:layout_constraintStart_toStartOf="@+id/NameReview"
        app:layout_constraintTop_toBottomOf="@+id/NameReview" />

    <ImageView
        android:id="@+id/header_bg3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/rectangle_1" />

    <ImageView
        android:id="@+id/header_map3"
        android:layout_width="107dp"
        android:layout_height="55dp"
        app:layout_constraintBottom_toBottomOf="@+id/header_bg3"
        app:layout_constraintEnd_toEndOf="@+id/header_bg3"
        app:layout_constraintHorizontal_bias="0.46"
        app:layout_constraintStart_toStartOf="@+id/header_bg3"
        app:layout_constraintTop_toTopOf="@+id/header_bg3"
        app:layout_constraintVertical_bias="0.592"
        app:srcCompat="@drawable/map" />

    <ImageView
        android:id="@+id/header_e3"
        android:layout_width="26dp"
        android:layout_height="42dp"
        app:layout_constraintEnd_toStartOf="@+id/header_map3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/header_map3"
        app:layout_constraintTop_toTopOf="@+id/header_map3"
        app:srcCompat="@drawable/e" />

    <RatingBar
        android:id="@+id/ratingBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:backgroundTint="#FFEB3B"
        android:foregroundTint="#FFEB3B"
        android:indeterminateTint="#FFEB3B"
        android:numStars="5"
        android:progressTint="#FFEB3B"
        android:rating="5.0"
        android:scaleX="1.3"
        android:scaleY="1.3"
        android:secondaryProgressTint="#FFEB3B"
        android:stepSize="0.5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AddressReview" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header_bg3"
        app:srcCompat="@drawable/header" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="REVIEW"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/header_bg3" />

    <TextView
        android:id="@+id/NameReview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="sans-serif-black"
        android:text="ChargePoint Chargin Station"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/textInputEditText"
        android:layout_width="323dp"
        android:layout_height="258dp"
        android:background="@null"
        android:gravity="top"
        android:hint="Write a comment here!"
        app:layout_constraintBottom_toBottomOf="@+id/imageView12"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:layout_constraintStart_toStartOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="@+id/imageView12" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.53"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ratingBar"
        app:srcCompat="@drawable/reviewborder" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.97"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_camera_alt_black_24dp" />

</androidx.constraintlayout.widget.ConstraintLayout>