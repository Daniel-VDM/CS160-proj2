<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Detail">

    <ImageView
        android:id="@+id/header_bg2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/rectangle_1" />

    <ImageView
        android:id="@+id/header_map2"
        android:layout_width="107dp"
        android:layout_height="55dp"
        app:layout_constraintBottom_toBottomOf="@+id/header_bg2"
        app:layout_constraintEnd_toEndOf="@+id/header_bg2"
        app:layout_constraintHorizontal_bias="0.44"
        app:layout_constraintStart_toStartOf="@+id/header_bg2"
        app:layout_constraintTop_toTopOf="@+id/header_bg2"
        app:layout_constraintVertical_bias="0.592"
        app:srcCompat="@drawable/map" />

    <ImageView
        android:id="@+id/header_e2"
        android:layout_width="26dp"
        android:layout_height="42dp"
        app:layout_constraintEnd_toStartOf="@+id/header_map2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/header_map2"
        app:layout_constraintTop_toTopOf="@+id/header_map2"
        app:srcCompat="@drawable/e" />

</androidx.constraintlayout.widget.ConstraintLayout>